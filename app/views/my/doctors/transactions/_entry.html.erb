
  <div class="weui-panel__bd">
      <div class="weui-media-box weui-media-box_text">
					<h4 class="weui-media-box__title">
            <%- if res.withdraw? %>
              提现
            <% else %>
              收入
            <% end %>
            <%= res.amount %>元
          </h4>
          <p class="weui-media-box__desc">来自
            <%- if res.withdraw? %>
              提现
            <% else %>
              预约
            <% end %>
            </p>
          <ul class="weui-media-box__info">
            <li class="weui-media-box__info__meta"><%= res.created_at.strftime('%Y.%m.%d %H:%M') %></li>
          </ul>
      </div>
			<div class="weui_media_status">
				<span>
          <%- if res.pending? %>
            结算中
          <% elsif res.settled? && res.operation.withdraw? %>
            已提现
          <% else  %>
            已结算
          <% end %>
				</span>
			</div>
  </div>
