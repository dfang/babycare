<%- if current_user.has_valid_contracts? %>
  <%= render 'doctors/statuses/has_valid_contracts' %>
<% elsif current_user.has_expired_contracts? %>
  <%= render 'doctors/statuses/has_expired_contracts' %>
<% elsif current_doctor.blank? %>
  <%= render 'doctors/statuses/welcome' %>
<% elsif current_doctor.verified? %>
  <%= render 'doctors/statuses/verified' %>
<% elsif current_doctor.denied? %>
  <%= render 'doctors/statuses/denied' %>
<% elsif current_doctor.pending? %>
  <%= render 'doctors/statuses/pending' %>
<% end %>
