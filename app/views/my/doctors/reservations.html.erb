<div class="tabbar">
  <div class="weui-tab">
      <div class="weui-tab_bd">

          <div class="panel">
            <div class="bd">
              <% if @reservations.any? %>
                <% @reservations.each do |res| %>
                  <div class="weui_panel">
                    <%= render 'entry', res: res %>
                  </div>
                <% end %>
              <% else %>
              你暂时没有预约
              <% end %>
            </div>
          </div>

      </div>
      <div class="weui-tabbar">
          <a href="/my/doctors/reservations" class="weui-tabbar__item weui_bar_item_on">
              <div class="weui-tabbar__icon">
                  <img src="/icon_nav_button.png" alt="">
              </div>
              <p class="weui-tabbar__label">我的预约</p>
          </a>
          <a href="/posts" class="weui-tabbar__item">
              <div class="weui-tabbar__icon">
                  <img src="/icon_nav_article.png" alt="">
              </div>
              <p class="weui-tabbar__label">必读文章</p>
          </a>
          <a href="/my/doctors/" class="weui-tabbar__item">
              <div class="weui-tabbar__icon">
                  <img src="/icon_nav_cell.png" alt="">
              </div>
              <p class="weui-tabbar__label">我</p>
          </a>
      </div>
  </div>
</div>
