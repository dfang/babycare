<div class="weui-cells">
  <div class="weui-cell">
    <div class="weui-cell__hd" style="position: relative;margin-right: 10px;">
        <img src="data:image/png;base64," alt="" style="width:60px;margin-right:5px;display:block">
        <p><%= @reservation.family_member.name %></p>
    </div>
    <div class="weui-cell__bd">
      <div class="weui_media_status">
        <%- if @reservation.patient_has_rated? %>
          已评价
        <% else %>
          <%= @reservation.aasm_state_text %>
        <% end %>
      </div>
    </div>
    <div class="weui-cell__hd" style="position: relative;margin-right: 10px;">
        <img src="data:image/png;base64," alt="" style="width:60px;margin-right:5px;display:block">
        <p>
          <% if @reservation.doctor.present? %>
            <%= @reservation.doctor_user_name %>
          <% else %>
            未指定
          <% end %>
        </p>
    </div>
  </div>
</div>

<div class="weui-cells">
  <div class="weui-cell">
    <div class="weui-cell__bd">
        <p>预约时间</p>
    </div>
    <div class="weui-cell__ft"><%= @reservation.reservation_date %></div>
  </div>

  <div class="weui-cell">
    <div class="weui-cell__bd">
        <p>预约地点</p>
    </div>
    <div class="weui-cell__ft">
      <% if @reservation.reservation_location.present? %>
        <%= @reservation.reservation_location %>
      <% else %>
        待定
      <% end %>
    </div>
  </div>

  <div class="weui-cell">
    <div class="weui-cell__bd">
        <p>症状描述</p>
    </div>
    <div class="weui-cell__hd"><img src="data:image/png;base64," alt="" style="width:58px;margin-right:5px;display:block"></div>
    <div class="weui-cell__ft"><%= @reservation.chief_complains %></div>
  </div>
</div>

<div class="weui-btn-area">
  <a href="javascript:;" class="weui-btn weui-btn_default">取消预约</a>
</div>
